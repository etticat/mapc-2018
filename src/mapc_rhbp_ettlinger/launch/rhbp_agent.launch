<launch>
    <arg name="agent_name" default="a1" />
    <arg name="bid_for_auctions" default="False"  />

    <node pkg="mapc_rhbp_ettlinger" type="rhbp_agent.py"
        name="rhbp_agent_$(arg agent_name)" output="screen"
        >
            <param name="agent_name" value="$(arg agent_name)" />
            <param name="bid_for_auctions" value="$(arg bid_for_auctions)" type="bool" />
            <param name="situationBias" type="double" value="0.7" />
            <param name="predecessorBias" type="double" value="1.0" />
            <param name="successorBias" type="double" value="1.0" />
            <param name="conflictorBias" type="double" value="1.0" />
            <param name="goalBias" type="double" value="0.7" />
            <param name="planBias" type="double" value="0.3" />
            <param name="activationThreshold" type="double" value="7.0" />
            <param name="createLogFiles" type="bool" value="false"/>
            <param name="activation_algorithm" type="string" value="default"/>
            <param name="max_parallel_behaviours" type="int" value="1"/> <!--Comment or negative for unlimited-->


            <param name="ShouldBidForAssembly.WEIGHT_LOAD" type="double" value="30"/>
            <param name="ShouldBidForAssembly.WEIGHT_INGREDIENT_LOAD" type="double" value="100"/>
            <param name="ShouldBidForAssembly.WEIGHT_STEPS" type="double" value="-3"/>
            <param name="ShouldBidForAssembly.ACTIVATION_THRESHOLD" type="double" value="0"/>

            <param name="AssemblyCombinationDecision.PRIORITY_EXPONENT" type="double" value="2"/>
            <param name="AssemblyCombinationDecision.WEIGHT_AFTER_ASSEMBLY_ITEM_COUNT"  value="-2.1" type="double"/>
            <param name="AssemblyCombinationDecision.WEIGHT_PRIORITY"  value="10" type="double"/>
            <param name="AssemblyCombinationDecision.WEIGHT_NUMBER_OF_AGENTS"  value="-10" type="double"/>
            <param name="AssemblyCombinationDecision.WEIGHT_PRIORITISATION_ACTIVATION"  value="30" type="double"/>
            <param name="AssemblyCombinationDecision.WEIGHT_IDLE_STEPS"  value="-3" type="double"/>
            <param name="AssemblyCombinationDecision.WEIGHT_MAX_STEP_COUNT"  value="-10" type="double"/>
            <param name="AssemblyCombinationDecision.MAX_AGENTS"  value="7" type="int"/>
            <param name="AssemblyCombinationDecision.MIN_AGENTS"  value="1" type="int"/>
            <param name="AssemblyCombinationDecision.ACTIVATION_THRESHOLD"  value="-100" type="double"/>

            <param name="ChooseStorageForHoardingMechanism.WEIGHT_STEPS"  value="-1" type="double"/>
            <param name="ChooseStorageForHoardingMechanism.WEIGHT_ITEMS_ALREADY_THERE"  value="-1" type="double"/>

            <param name="GatherDecisionMechanism.WEIGHT_STEPS"  value="1" type="double"/>
            <param name="GatherDecisionMechanism.WEIGHT_ASSEMBLY_ROLE_MATCH"  value="2.5" type="double"/>
            <param name="GatherDecisionMechanism.WEIGHT_ASSEMBLY_ROLE_MATCH_COUNT"  value="0.5" type="double"/>
            <param name="GatherDecisionMechanism.WEIGHT_PRIORITY"  value="200" type="double"/>


            <param name="JobDecider.DEFAULT_FINISHED_PRODUCT_GOAL"  value="5" type="double"/>
            <param name="JobDecider.BID_PERCENTILE"  value="50" type="double"/>
            <param name="JobDecider.ACTIVATION_THRESHOLD"  value="-50" type="double"/>
            <param name="JobDecider.IMPORTANT_JOB_THRESHOLD"  value="0" type="double"/>
            <param name="JobDecider.ACTIVATION_TO_DESIRED_PRODUCT_CONVERSION"  value="0.2" type="double"/>
            <param name="JobDecider.WEIGHT_PERCENTILE"  value="10" type="double"/>
            <param name="JobDecider.WEIGHT_TIME_PASSED"  value="-0.3" type="double"/>
            <param name="JobDecider.TIME_LEFT_WEIGHT_START"  value="30" type="double"/>
            <param name="JobDecider.WEIGHT_TIME_OVER"  value="-0.4" type="double"/>

            <param name="ChooseBestJobCombination.MIN_STEP_BUFFER"  value="5" type="double"/>
            <param name="ChooseBestJobCombination.WEIGHT_LOAD"  value="8" type="double"/>
            <param name="ChooseBestJobCombination.WEIGHT_INGREDIENT_LOAD"  value="8" type="double"/>
            <param name="ChooseBestJobCombination.WEIGHT_NO_STORAGE_NEEDED"  value="20" type="double"/>
            <param name="ChooseBestJobCombination.WEIGHT_STEPS"  value="-1" type="double"/>
            <param name="ChooseBestJobCombination.ACTIVATION_THRESHOLD"  value="-1000" type="double"/>
    </node>

</launch>
